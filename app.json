{
  "expo": {
    "name": "fintech",
    "slug": "fintech",
    "version": "2.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/dollars-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.none.fintech"
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dollars-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.none.fintech",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/dollars-icon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://bobobob.dev/"
        }
      ],
      "expo-asset",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "01c2991b-7656-4302-8927-2ec996a9799b"
      }
    }
  }
}
